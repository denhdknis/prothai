<div class="img-fluid pic_top">
    <div class="col-xs-12 cover-img">
      <div class="cover-text text-center">
        <div><p>Mash Up</p></div>
        <div class="cover-subtext"><p>海外インターンを通して人生に新たな1ページを！</p></div>
      </div>
    </div>
</div>
    <div class="container all_shadow sm_top">
                  <h2 class="header_text">
                   <%= fa_icon("file-user") %>社長インタビュー一覧
                  </h2>
                  <div class="main_contents">
                   <div class="row">
                     <% @products.each do |product| %>
                        <div class="col-sm-6 col-md-4 search" data-category='CMS' data-tag='["<%= product.country.name %>","<%= product.period.id %>"]'>
                      <a href="/products/<%= product.id %>">
                        <div class="card top20">
                        <div class="sub_pic">
                           <img class="img-thumbnail" src="<%= product.slider_photo1 %>" alt="">
                           <span><%= product.country.name %></span>
                        </div>
                         <div class="card-body">
                            <div class="card-title_top">
                               <%= truncate(product.c_title, length: 58) do %>
                               <% end %>
                            </div>
                             <div class="card-title">
                               <table class="media-card_table">
                                  <tr class="card_C_name">
                                   <th>会社名</th>
                                     <td>
                                      <%= truncate(product.c_name, length: 30) do %>
                                      <% end %>
                                     </td>
                                   </tr>
                               </table>
                             </div>
                             <% product.connect_tags.each_with_index do |connect_tag,i| %>
                              <li class="tag_zone_index">
                               <%= connect_tag.tag.name %>
                               </li>
                               <%  break if i== 2 %>
                             <% end %>
                          </div>
                        </div>
                      </a>
                      <!--/.Card-->
                      </div>
                     <% end %>
                    <!--Grid column-->
                    </div>
                  </div>
                   <a href="products"><div class="btn btn-lg btn-block index_page_btn margin_btn">他の社長インタビューを見に行く</div></a>
    </div>

